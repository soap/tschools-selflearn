{
    "name": "soap/tschools-selflearn",
    "type": "project",
    "version": "0.0.1",
    "description": "Online teachable platform",
    "keywords": ["framework", "laravel", "online", "learning"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "cybercog/laravel-ownership": "^5.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.5",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/nova": "^3.30",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "marshmallow/nova-field-string-generator": "^1.1",
        "michielkempen/nova-order-field": "^2.0",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/line": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "spatie/laravel-enum": "^2.5",
        "spatie/laravel-permission": "^5.4",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-tags": "^3.1",
        "stevelacey/nova-vimeo-field": "^2.0",
        "tightenco/ziggy": "^1.0",
        "vimeo/laravel": "^5.6",
        "wulfheart/pretty_routes": "^0.3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
